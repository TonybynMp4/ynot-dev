		"@types/react-dom": "^19.1.9",
		"@typescript-eslint/eslint-plugin": "^8.44.1",
		"@typescript-eslint/parser": "^8.44.1",
		"drizzle-kit": "^0.31.5",
		"eslint": "^9.36.0",
		"eslint-config-next": "^15.5.4",
		"eslint-plugin-drizzle": "^0.2.3",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"tailwindcss": "^4.1.13",
