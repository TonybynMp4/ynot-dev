		"mysql2": "^3.15.1",
		"next": "^15.5.4",
		"next-intl": "^4.3.9",
		"next-themes": "^0.4.6",
		"radix-ui": "^1.4.3",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"tailwind-merge": "^3.3.1",
